{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": [
      "error",
      "unix"
    ],
    "camelcase": 0,
    "prefer-promise-reject-errors": 0,
    "react/prop-types": 0
  }
}
