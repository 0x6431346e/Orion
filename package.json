{
  "name": "lumpy",
  "version": "0.1.1",
  "description": "Lumpy is an easy-to-use and KISS IPFS desktop interface",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "start": "electron .",
    "build-dev": "electron-packager . Lumpy --overwrite --out=./build",
    "build-icons": "icon-gen -i ./docs/logo.svg -o ./build/ -m ico,icns,png",
    "build-app": "electron-packager . Lumpy --overwrite --icon=./build/app.icns --out=./build",
    "build": "yarn build-icons ; yarn build-app"
  },
  "repository": "https://github.com/siderus/Lumpy",
  "keywords": [
    "electron",
    "ipfs",
    "file",
    "p2p",
    "upload"
  ],
  "author": "Lorenzo Setale",
  "license": "GPL-3.0",
  "devDependencies": {
    "electron-packager": "^8.5.1",
    "electron-react-devtools": "^0.5.3",
    "eslint": "^3.14.0",
    "eslint-config-equimper": "^1.3.5",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "icon-gen": "^1.0.7",
    "jest": "^22.4.2",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "byte-size": "^3.0.0",
    "classnames": "^2.2.5",
    "electron": "^1.6.0",
    "electron-compile": "^5.1.3",
    "electron-compilers": "^5.4.0",
    "electron-settings": "^2.2.2",
    "ipfs-api": "git+https://github.com/ipfs/js-ipfs-api.git",
    "is-electron-renderer": "^2.0.1",
    "is-ipfs": "^0.3.0",
    "mobx": "^3.0.2",
    "mobx-react": "^4.1.0",
    "multiaddr": "^3.0.2",
    "nedb": "^1.8.0",
    "photon": "git+https://github.com/connors/photon.git",
    "promised-exec": "^1.0.1",
    "q-sqlite3": "^0.1.3",
    "query-string": "^5.1.0",
    "react": "^15.4.2",
    "react-desktop": "^0.2.17",
    "react-dom": "^15.4.2",
    "react-photonkit": "git+https://github.com/react-photonkit/react-photonkit.git",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "styled-components": "^3.1.6",
    "underscore": "^1.8.3"
  }
}
